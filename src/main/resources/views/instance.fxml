<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="568.0" prefWidth="567.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.usthb.dmtk.controllers.InstanceController">
   <children>
       <VBox prefHeight="95.0" prefWidth="557.0">
           <VBox.margin>
               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
           </VBox.margin>
           <Label text="Relation Information" />
           <Label fx:id="relationName" text="name: ">
               <VBox.margin>
                   <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
               </VBox.margin>
           </Label>
           <Label fx:id="numInstances" text="instances: ">
               <VBox.margin>
                   <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
               </VBox.margin>
           </Label>
           <Label fx:id="numAttributes" text="attributes: ">
               <VBox.margin>
                   <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
               </VBox.margin>
           </Label>
       </VBox>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
         <children>
            <Label text="Instances" />
            <TableView fx:id="instances" prefHeight="200.0" prefWidth="200.0">
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </VBox>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
             <Label text="Attributes: ">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </Label>
             <TableView fx:id="attributes" prefHeight="325.0" prefWidth="557.0">
               <columns>
                  <TableColumn fx:id="name" prefWidth="89.0" text="name" />
                  <TableColumn fx:id="type" minWidth="3.0" prefWidth="61.0" text="type" />
               </columns>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </VBox>
   </children>
</VBox>
